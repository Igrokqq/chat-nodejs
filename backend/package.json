{
   "name": "zephyr",
   "scripts": {
      "build": "rimraf dist && tsc -p tsconfig.json",
      "dev": "npx nodemon",
      "migration:run:dev": "ts-node -r dotenv/config ./node_modules/.bin/typeorm/ migration:run --config ormconfig.ts -- dotenv_config_path=.env.dev",
      "migration:revert:dev": "ts-node -r dotenv/config ./node_modules/.bin/typeorm/ migration:revert --config ormconfig.ts -- dotenv_config_path=.env.dev",
      "start": "ts-node src/index.ts"
   },
   "author": {
      "email": "wimmmly@gmail.com",
      "name": "Pavel"
   },
   "license": "MIT",
   "devDependencies": {
      "@types/body-parser": "1.19.1",
      "@types/cors": "2.8.12",
      "@types/express": "4.17.13",
      "@types/helmet": "4.0.0",
      "@types/http-errors": "1.8.1",
      "@types/node": "14",
      "@types/socket.io": "3.0.2",
      "@typescript-eslint/eslint-plugin": "4.28.2",
      "@typescript-eslint/parser": "4.28.2",
      "class-transformer": "0.4.0",
      "class-validator": "0.13.1",
      "dotenv": "10.0.0",
      "eslint": "7.30.0",
      "eslint-config-prettier": "8.3.0",
      "eslint-plugin-jest": "24.3.6",
      "eslint-plugin-prettier": "3.4.0",
      "module-alias": "2.2.2",
      "nodemon": "2.0.12",
      "prettier": "2.3.2",
      "reflect-metadata": "0.1.13",
      "rimraf": "3.0.2",
      "ts-node": "10.1.0",
      "typescript": "4.3.5"
   },
   "dependencies": {
      "compression": "1.7.4",
      "cors": "2.8.5",
      "express": "4.17.1",
      "http-errors": "1.8.0",
      "joi": "17.4.1",
      "morgan": "1.10.0",
      "pg": "8.7.1",
      "socket.io": "3.0.2",
      "typeorm": "0.2.36"
   },
   "_moduleAliases": {
      "@components": "./dist/components",
      "@common": "./dist/common"
   }
}
